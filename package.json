    "eslint-config",
    "config",
    "styleguide",
    "frontend",
    "javascript",
    "es5",
    "es6",
    "nanodegree"
  ],
  "author": "Linesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linesh-simplicity/eslint-config-udacity/issues"
  },
  "homepage": "https://github.com/linesh-simplicity/eslint-config-udacity#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-espower": "^2.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "iconv-lite": "^0.4.17",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.2"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  }
