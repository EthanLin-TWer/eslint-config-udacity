{
  "name": "eslint-config-udacity",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --require babel-register test",
    "bump:patch": "npm version patch -m \"[$(git config user.name)] ðŸ”–Version: patch upgrade to %s and published to npm\"",
    "bump:minor": "npm version minor -m \"[$(git config user.name)] ðŸ”–Version: minor upgrade to %s and published to npm\"",
    "bump:major": "npm version major -m \"[$(git config user.name)] ðŸ”–Version: major upgrade to %s and published to npm\"",
    "publish:patch": "npm run bump:patch && npm publish",
    "publish:minor": "npm run bump:minor && npm publish",
    "publish:major": "npm run bump:major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linesh-simplicity/eslint-config-udacity.git"
  },
  "keywords": [
